{% extends 'base.html.twig' %}

{% block body %}
    <h1>Imagesannuaire</h1>

    <table>
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ imagesAnnuaire.id }}</td>
            </tr>
            <tr>
                <th>Url</th>
                <td>{{ imagesAnnuaire.url }}</td>
            </tr>
            <tr>
                <th>Infront</th>
                <td>{% if imagesAnnuaire.inFront %}Yes{% else %}No{% endif %}</td>
            </tr>
        </tbody>
    </table>

    <ul>
        <li>
            <a href="{{ path('imagesannuaire_index') }}">Back to the list</a>
        </li>
        <li>
            <a href="{{ path('imagesannuaire_edit', { 'id': imagesAnnuaire.id }) }}">Edit</a>
        </li>
        <li>
            {{ form_start(delete_form) }}
                <input type="submit" value="Delete">
            {{ form_end(delete_form) }}
        </li>
    </ul>
{% endblock %}
